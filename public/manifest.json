{
  "manifest_version": 3,
  "name": "Canvas Archiver Extension",
  "version": "1.0.3",
  "description": "Archive files in your canvas courses!",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Canvas Archiver Extension",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "host_permissions": [
    "https://*.instructure.com/*",
    "https://*.edu/*"
  ],
  "permissions": [
    "downloads",
    "declarativeContent",
    "tabs",
    "activeTab"
  ]
}